//Count alphabets in the string
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String str = sc.nextLine();
		int ct = 0;
		for(int i=0; i<str.length(); i++){
		    char c = str.charAt(i);
		    if(c>='a'&&c<='z')
		        ct++;
		}
		System.out.println(ct);
	}
}
